<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <managed-bean>
        <description>Administrador de marcadores</description>
        <managed-bean-name>markerBean</managed-bean-name>
        <managed-bean-class>com.encoming.encoming.presentation.controller.PointBean</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/admin.xhtml</from-view-id>
        <from-outcome>addVehicle</from-outcome>
        <to-view-id>/addVehicle.xhtml</to-view-id>
    </navigation-rule>
</faces-config>
